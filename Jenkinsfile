pipeline
{
  agent any
  stages
  {
    stage("build")
    {
      steps
      {
        echo "building your project..."
      }
    }
    stage("test")
    {
      steps
      {
        echo "testing your project..."
      }
    }
    stage("deploy")
    {
      steps
      {
        echo "deploying your project..."
      }
    }
  }
}
